<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	 <class name="base.Usuario" table="USUARIO"> 
 <id column ="usuario" name = "usuario" type ="string"/>
	<property column ="contraseña" name="contraseña" not-null="true" type="string"/>
    <property column ="puntaje" name="puntaje" not-null="true" type="int"/>
 </class>
</hibernate-mapping>
